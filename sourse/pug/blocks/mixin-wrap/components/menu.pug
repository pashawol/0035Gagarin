//- WP menu
mixin mnu()
	-
		var nav= {
			"О нас":"#",
			"Преимущества  ":"#",
			"Отзывы":"#",
			"Как стать кайтером ":"#",
			"Курсы и цены":"#",
			"Инструкторы":"#",
			"Станция":"#",
			"Досуг":"#", 
			"Питание":"#", 
			"Кайт спот":"#", 
			"Аренда":"#", 
			"Услуги":"#", 
			"Проживание":"#", 
			"Как добраться":"#", 
			"Блог":"#", 
			"Контакты":"#", 
			}
	ul.menu(itemscope itemtype="http://schema.org/SiteNavigationElement")
		each linkPatch, linkName in nav
			li.menu-item(itemprop="item" ): a(href= linkPatch itemprop="url")= linkName

//- menu
//- /WP menu



