//- WP menu
mixin mnu()
	-
		var nav= {
			"О нас":"#sAbout",
			"Преимущества  ":"#sAdvantages",
			"Отзывы":"#sRews",
			"Как стать кайтером ":"#sSteps",
			"Курсы и цены":"#sPrices",
			"Инструкторы":"#sInstructors",
			"Станция":"#sSertificates",
			"Досуг":"#sKiteStation", 
			"Питание":"#sLeisure", 
			"Кайт спот":"#sCuisine", 
			"Аренда":"#sPlace", 
			"Услуги":"#sRent", 
			"Проживание":"#sAdditionalServises", 
			"Как добраться":"#sAccommodation", 
			"Блог":"#sBlog", 
			"Контакты":"#sContact", 
			}
	ul.menu(itemscope itemtype="http://schema.org/SiteNavigationElement")
		each linkPatch, linkName in nav
			li.menu-item(itemprop="item" ): a(href= linkPatch itemprop="url")= linkName

//- menu
//- /WP menu



